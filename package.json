{
  "name": "svelte-jest",
  "version": "0.3.1",
  "description": "Jest Svelte component transformer",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktsn/svelte-jest.git"
  },
  "keywords": [
    "Svelte",
    "Jest",
    "transformer",
    "test",
    "component"
  ],
  "author": "katashin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ktsn/svelte-jest/issues"
  },
  "homepage": "https://github.com/ktsn/svelte-jest#readme",
  "jest": {
    "transform": {
      "\\.js$": "babel-jest",
      "\\.svelte$": "./lib/index.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "svelte"
    ],
    "testRegex": "/test/.+\\.spec\\.js$"
  },
  "scripts": {
    "lint": "eslint lib test && prettier -l \"{lib,test}/**/*.js\"",
    "format": "prettier --write \"{lib,test}/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "devDependencies": {
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^7.3.0",
    "eslint-config-ktsn": "^2.0.1",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.0.1",
    "prettier": "2.1.2",
    "svelte": "^3.23.2"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  }
}
